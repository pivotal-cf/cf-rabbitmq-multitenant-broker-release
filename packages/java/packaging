# abort script on any command that exit with a non zero value
set -e

JAVA_VERSION="11.0.2_07"

archive="java/openjdk-${JAVA_VERSION}.tar.gz"

echo "Extracting archive..."
mkdir openjdk-${JAVA_VERSION}/
tar xzf $archive -C openjdk-${JAVA_VERSION}/

cp -R openjdk-${JAVA_VERSION}/* ${BOSH_INSTALL_TARGET}
